<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="top.zzh.dao.CashTypeDAO">

	<select id="getByPid" parameterType="long" resultType="top.zzh.vo.CashTypeVO">
		<![CDATA[
			select id, type from t_cash_type where pid = #{pid}
		]]>
	</select>
	 
</mapper>